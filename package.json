{
  "name": "spark-react",
  "version": "3.3.1",
  "description": "Spark - Admin & Dashboard Template [ReactJS]",
  "author": "Paul Laros <support@bootlab.io>",
  "private": true,
  "scripts": {
    "sass:compile": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "react-scripts build",
    "watch:css": "chokidar \"src/assets/scss/**/*\" -c \"npm run build:css\"",
    "watch:js": "react-scripts start",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-brands-svg-icons": "5.13.0",
    "@fortawesome/free-regular-svg-icons": "5.13.0",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@fortawesome/react-fontawesome": "0.1.9",
    "apexcharts": "3.18.1",
    "availity-reactstrap-validation": "2.6.1",
    "bootstrap": "4.4.1",
    "chart.js": "2.9.3",
    "chokidar-cli": "2.1.0",
    "classnames": "2.2.6",
    "fullcalendar": "3.10.1",
    "google-map-react": "1.1.7",
    "ionicons": "4.6.3",
    "jquery": "3.4.1",
    "jvectormap-next": "3.0.0",
    "moment": "2.24.0",
    "node-sass": "^4.13.1",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "4.1.5",
    "postcss-cli": "7.1.0",
    "react": "16.13.1",
    "react-apexcharts": "1.3.7",
    "react-app-polyfill": "1.0.6",
    "react-bootstrap-table-next": "4.0.1",
    "react-bootstrap-table2-paginator": "2.1.2",
    "react-bootstrap-table2-toolkit": "2.1.2",
    "react-chartjs-2": "2.9.0",
    "react-datetime": "^3.0.0-beta.8",
    "react-dom": "16.13.1",
    "react-dragula": "1.1.17",
    "react-feather": "2.0.4",
    "react-input-mask": "2.0.4",
    "react-ionicons": "^3.1.4",
    "react-popper": "2.2.2",
    "react-quill": "1.3.5",
    "react-redux": "7.2.0",
    "react-redux-toastr": "7.6.4",
    "react-router-dom": "5.1.2",
    "react-scripts": "^3.4.1",
    "react-select": "3.1.0",
    "react-transition-group": "4.3.0",
    "reactstrap": "8.4.1",
    "redux": "4.0.5"
  },
  "devDependencies": {}
}
